function C_Div(const Z1 : Complex; const Z2 : Complex):Complex;
var
    A : Double;
    B : Double;
    C : Double;
    D : Double;
    E : Double;
    F : Double;
begin
    A := Z1.X;
    B := Z1.Y;
    C := Z2.X;
    D := Z2.Y;
    if Abs(D)<Abs(C) then
    begin
        E := D/C;
        F := C+D*E;
        C_Div.X := (A+B*E)/F;
        C_Div.Y := (B-A*E)/F;
    end
    else
    begin
        E := C/D;
        F := D+C*E;
        C_Div.X := (B+A*E)/F;
        C_Div.Y := (-A+B*E)/F;
    end;
end;


function C_DivR(const Z1 : Complex; const R : Double):Complex;
begin
    C_DivR.X := Z1.X/R;
    C_DivR.Y := Z1.Y/R;
end;


function C_RDiv(const R : Double; const Z2 : Complex):Complex;
var
    A : Double;
    C : Double;
    D : Double;
    E : Double;
    F : Double;
begin
    A := R;
    C := Z2.X;
    D := Z2.Y;
    if Abs(D)<Abs(C) then
    begin
        E := D/C;
        F := C+D*E;
        C_RDiv.X := A/F;
        C_RDiv.Y := -A*E/F;
    end
    else
    begin
        E := C/D;
        F := D+C*E;
        C_RDiv.X := A*E/F;
        C_RDiv.Y := -A/F;
    end;
end;


