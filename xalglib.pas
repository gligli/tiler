function C_Mul(const Z1 : Complex; const Z2 : Complex):Complex;
begin
    C_Mul.X := Z1.X*Z2.X-Z1.Y*Z2.Y;
    C_Mul.Y := Z1.X*Z2.Y+Z1.Y*Z2.X;
end;


