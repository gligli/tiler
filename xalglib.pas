begin
    Result.c := Clone(src.c);
    Result.rmserror := src.rmserror;
    Result.avgerror := src.avgerror;
    Result.avgrelerror := src.avgrelerror;
    Result.cvrmserror := src.cvrmserror;
    Result.cvavgerror := src.cvavgerror;
